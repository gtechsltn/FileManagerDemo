    <client>
      <endpoint address="net.tcp://localhost:998/FileManagerService"
                binding="netTcpBinding"
                bindingConfiguration="netTcp"
                contract="SharedLibrary.IFileManagerService"
                name="NetTcpBinding_IFileManagerService" />
